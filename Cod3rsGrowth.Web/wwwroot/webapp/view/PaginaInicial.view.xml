<mvc:View controllerName="ui5.coders.controller.PaginaInicial"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc">
    <Panel headerText="{i18n>tituloPainelOla}">
        <content>
            <Button id="botaoSayHello" text="{i18n>exibirTextoBotaoOla}" press=".exibirOla"/>
            <Input value="{/recipient/name}" description="Hello {/recipient/name}" valueLiveUpdate="true" width="60%"/>
        </content>
    </Panel>
</mvc:View>