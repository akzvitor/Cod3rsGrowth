<mvc:View controllerName="ui5.coders.controller.Listagem"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc">
    <Panel headerText="{i18n>Listagem.titulo}">
        <content>
            <Table
                id="TabelaCompras"
                items="{restCompras>/}">
                <columns>
                    <Column>
                        <Text text="Aaaaaa" />
                    </Column>
                    <Column>
                        <Text text="Bbbbbb" />
                    </Column>
                </columns>
                <items>
                    <ColumnListItem>
                        <cells>
                            <Text text="{restCompras>id}"/>
                            <Text text="{restCompras>email}"/>
                        </cells>
                    </ColumnListItem>
                </items>
            </Table>    
        </content>
    </Panel>
</mvc:View>