{
    "_version": "1.58.0",
    "sap.app": {
        "id": "ui5.coders",
        "i18n": "i18n/i18n.properties",
        "title": "{{App.titulo}}",
        "description": "{{App.descricao}}",
        "type": "application",
        "applicationVersion": {
            "version": "1.0.0"
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "dependencies": {
            "minUI5Version": "1.108.0",
            "libs": {
                "sap.ui.core": {},
                "sap.m": {}
            }
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "ui5.coders.i18n.i18n",
                    "supportedLocales": [
                        ""
                    ],
                    "fallbackLocale": ""
                }
            }
        },
        "rootView": {
            "viewName": "ui5.coders.view.App",
            "type": "XML",
            "id": "app"
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "type": "View",
                "viewType": "XML",
                "path": "ui5.coders.view",
                "controlId": "app",
                "controlAggregation": "pages",
                "transition": "slide",
                "bypassed": {
                    "target": "notFound"
                },
                "async": true
            },
            "routes": [
                {
                    "pattern": "",
                    "name": "paginainicial",
                    "target": "paginainicial"
                },
                {
                    "pattern": "criacaoCompra",
                    "name": "criacaoCompra",
                    "target": "criacaoCompra"
                }
            ],
            "targets": {
                "paginainicial": {
                    "id": "paginainicial",
                    "name": "PaginaInicial"
                },
                "notFound": {
                    "id": "notFound",
                    "name": "NotFound",
                    "transition": "show"
                },
                "criacaoCompra": {
                    "id": "criacaoCompra",
                    "name": "CriacaoCompra"
                }
            }
        }
    }
}